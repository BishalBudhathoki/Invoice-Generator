                            <Label layoutX="246.0" layoutY="180.0" text="Street" />
                            <Label layoutX="242.0" layoutY="222.0" text="Suburb" />
                            <Label layoutX="248.0" layoutY="261.0" text="State" />
                            <Label layoutX="216.0" layoutY="299.0" text="Postal Code" />
